<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="50.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.test3.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label text="Login :" />
    <TextField fx:id="monID" />
    <Label text="Mot de passe : " />
    <TextField fx:id="monMDP" accessibleRole="PASSWORD_FIELD" />
    <Label fx:id="welcomeText" />
    <Button fx:id="monBoutonConnecter" onAction="#onHelloButtonClick" text="Connexion" />
    <Button id="inscription" onAction="#onInscription" mnemonicParsing="false" text="Inscription" />
</VBox>
