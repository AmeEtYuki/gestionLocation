    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label text="Login :" />
    <TextField fx:id="monID"/>
    <Label text="Mot de passe : " />
    <TextField fx:id="monMDP"/>
    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="Connexion"
            fx:id="monBoutonConnecter" />
</VBox>
